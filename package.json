{
  "name": "spirited",
  "version": "1.0.0-pre.4",
  "description": "A small library for animating numerical arrays",
  "main": "dist/spirited.bundle.js",
  "scripts": {
    "build": "webpack --progress --env=dist",
    "build-dev": "webpack --progress --env=dev",
    "todo": "leasot --reporter markdown src/**/*.js test/**/*.js > TODO.md; exit 0",
    "doc": "jsdoc --configure .jsdocrc",
    "lint": "eslint src/**/*.js",
    "cov": "BABEL_ENV=test rm -rf ./reports/coverage && babel-node ./node_modules/istanbul/lib/cli.js cover --dir ./reports/coverage _mocha -- --opts ./test/mocha.opts ./test",
    "test": "BABEL_ENV=test npm run lint && mocha --opts ./test/mocha.opts ./test",
    "test:watch": "npm run test -- -w",
    "build-browser-tests": "webpack --config webpack.tests.config.js --env=test",
    "build-browser-tests:watch": "npm run build-browser-tests -- --watch --env=test",
    "prepublish": "npm run build",
    "start": "webpack-dev-server --content-base example/ --compress --watch --env=dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/autopilothq/spirited.git"
  },
  "keywords": [],
  "author": "Autopilot Team <support@autopilothq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/autopilothq/spirited/issues"
  },
  "homepage": "https://github.com/autopilothq/spirited",
  "dependencies": {
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.7.4",
    "easing-utils": "0.0.5",
    "eventemitter3": "^2.0.0",
    "present": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "codeclimate-test-reporter": "^0.3.1",
    "docdash": "^0.4.0",
    "eslint": "^3.1.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-require-path-exists": "^1.1.5",
    "glob": "^7.0.6",
    "http-server": "^0.9.0",
    "istanbul": "^1.0.0-alpha.2",
    "jsdoc": "^3.4.0",
    "json-loader": "^0.5.3",
    "leasot": "^4.0.1",
    "lodash.merge": "^4.6.0",
    "minimist": "^1.2.0",
    "mocha": "^3.0.2",
    "mocha-clean": "^1.0.0",
    "prettyjson": "^1.1.3",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "webpack": "^2.1.0-beta.8",
    "webpack-dev-server": "^2.1.0-beta.0"
  }
}
